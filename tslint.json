{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": {
      "severity": "warn"
    },
    // Prettier conflicts
    "trailing-comma": false,
    "arrow-parens": false,
    // Not convenient
    "object-literal-sort-keys": false,
    "interface-name": false
  },
  "rulesDirectory": []
}

